HURDLE 3

def turn_right():
    turn_left()
    turn_left()
    turn_left()

def lompat():
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
while not at_goal():#diulangi selama tidak di goal
    if wall_in_front():
        lompat()
    else:
        move()
        
       
HURDLE 4

       def turn_right():
    turn_left()
    turn_left()
    turn_left()

def round_corner():
    turn_right()
move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
    
while not at_goal():#diulangi selama tidak di goal
    if wall_in_front():
        lompat()
    else:
        move()
